{
    "presets": [
        "es2015",
        "stage-2",
        "react"
    ],
    "plugins": [
        "transform-runtime",
        "transform-object-rest-spread",
        "transform-decorators-legacy", [
            "import",
            {
                "libraryName": "antd",
                "style": true
            }
        ],
        [
            "react-intl",
            {
                "messagesDir": "./src/i18n-messages"
            }

        ]
    ]
}